<div id="<%= dom_id question_option %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= question_option.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Is correct:</strong>
    <%= question_option.is_correct? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Question:</strong>
    <%= question_option.question_id %>
  </div>
</div>
