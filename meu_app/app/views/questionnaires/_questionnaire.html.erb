<div id="<%= dom_id questionnaire %>" class="w-full sm:w-auto my-5 space-y-5 bg-pink-50 border border-pink-200 rounded-3xl shadow-md p-5">
  <div>
    <strong class="block font-medium mb-1 text-pink-700">Código:</strong>
    <%= questionnaire.code %>
  </div>
  <div>
    <strong class="block font-medium mb-1 text-pink-700">Título:</strong>
    <%= questionnaire.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1 text-pink-700">Descrição:</strong>
    <%= questionnaire.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1 text-pink-700">Duração (minutos):</strong>
    <%= questionnaire.duration_minutes %>
  </div>
</div>
